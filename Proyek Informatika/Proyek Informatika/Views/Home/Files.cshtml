<script type="text/javascript">
    $(document).ready(function () {
        $.post("Home/GetFiles", "", function (data) {
            var get = "";
            $.each(data, function (index, value) {
                get += "<li><a id ='file' title ='"+value.id+"' href ='PengaturanFile/GetDownload?id="+value.id+"'>" + value.nama + "</a></li>";
            });
            $("#file_list").html(get);
        });
    });
    $("#file").click(function (evt) {
        alert("href: " );
        var href = $(this).attr('title');
        
        //window.open("PengaturanFile/GetDownload?id=" + href, "_blank");
    });
</script>

